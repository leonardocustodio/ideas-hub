# NuxtHub project key for deployment
NUXT_HUB_PROJECT_KEY=project-id

# Session Management
NUXT_SESSION_PASSWORD=your-32-character-or-longer-session-password-here

# SendGrid Configuration for OTP Email
SENDGRID_API_KEY=your-sendgrid-api-key-here
